{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::labelstudio/*",
      "Condition": {
        "StringEquals": {
          "s3:RequestOrigin": [
            "http://127.0.0.1:8081",
            "http://your-domain-name"
          ]
        }
      }
    }
  ]
}
